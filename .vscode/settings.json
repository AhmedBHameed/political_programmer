{
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeLens": true,
  "i18n-ally.localesPaths": "./i18n",
  "i18n-ally.theme.annotation": "#4bff00",
  "i18n-ally.displayLanguage": "ar",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "cSpell.words": [],
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  }
}
